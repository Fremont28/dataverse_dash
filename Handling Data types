# When you create a python script, it should have no spaces in the
# filename and the file extension should be ".py" rather than ".txt"

def main():

    test_data= "october"

def data_classifier(test_data):
    if isinstance(test_data,tuple): #(s,tuple)
        print('tuple') 
    if isinstance(test_data,str):
        print('str')
    if isinstance(test_data,float):
        print('float')
    if isinstance(test_data,bool):
        print('bool')
    else:
        print('') 

data_classifier(test_data) # this line should be inside the main function

if __name__ == '__main__':
    main()
